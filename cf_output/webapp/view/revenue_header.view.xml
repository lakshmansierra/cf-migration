					<vm:VariantManagement standardItemText="Unified DOI">
					</vm:VariantManagement>
				</f:heading>
				<f:actions>
					<ToolbarSpacer/>
					<Button
							text="ADD"
							icon="sap-icon://create-form"
							type="Transparent"
							 press="onDoiHeaderAddPress"/>
					<Button
							text="EDIT"
							icon="sap-icon://write-new-document"
							type="Transparent"
							press="onToggleFooter"/>
					<Button
