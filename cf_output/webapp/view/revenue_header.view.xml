									demandPopin="true">
								<Text text="Total Federal"/>
							</Column>
						<Column
								minScreenWidth="Tablet"
								demandPopin="true">
							<Text text="Total Indian"/>
						</Column>	
						 <!--newly added-->
					</columns>

					<items>
						<ColumnListItem
								vAlign="Middle">
							<cells>
								<!--<ObjectIdentifier-->
								<!--		title="{WellId}"-->
								<!--	/>-->
								<Text text="{CmpCode}"/>
								
								<Text text="{Venture}"/>
								<Text text="{Doi}"/>
