												<!--<Button id="ARPtoolMail" icon="sap-icon://email" tooltip="" type="Accept" press="onSendMail"/>-->
												<!--<ToolbarSpacer width="2em"/>-->
												<Button id="ARPAmanageForm" icon="sap-icon://edit" tooltip="Manage Form" type="Accept" press="onManageForm"/>
												<Button id="ARPAviewForm" icon="sap-icon://show" tooltip="View Form" type="Accept" press="onDisplayForm"/>
												<!--<Button id="ARPAdeleteForm" icon="sap-icon://delete" tooltip="Delete Form" type="Accept" press="onDisplayForm"/>-->
												<!--<Button id="ARPApublishForm" icon="sap-icon://add-activity-2" tooltip="Publish Form" type="Accept" press="onApproveForm"/>-->
												<Button id="ARPAtoolRevision" icon="sap-icon://duplicate" tooltip="Create Revision" type="Accept" press="onCreateRevision"/>
												<Button id="ARPAtoolCreate" icon="sap-icon://add" tooltip="Create Request" type="Accept" press="onCreateRequest"/>
												<Button id="ARPAtoolDelete" icon="sap-icon://delete" tooltip="Delete Request" type="Accept" press="onDeleteRequest"/>
											</content>
										</OverflowToolbar>
									</headerToolbar>
					<columns>
						<Column
								minScreenWidth="Tablet"
								demandPopin="true">
							<Text text="Company Code"/>
						</Column>
						<Column
								minScreenWidth="Tablet"
								demandPopin="true">
							<Text text="Venture"/>
						</Column>
						<Column
								minScreenWidth="Tablet"
								demandPopin="true">
							<Text text="DOI"/>
						</Column>
						 <!--newly added-->
								<Column
									minScreenWidth="Tablet"
									demandPopin="true">
								<Text text="Well ID"/>
							</Column>
							
								<Column
									minScreenWidth="Tablet"
									demandPopin="true">
								<Text text="Well Name"/>
							</Column>
							
								<Column
									minScreenWidth="Tablet"
									demandPopin="true">
								<Text text="Unified DOI Type"/>
							</Column>
							
								<Column
									minScreenWidth="Tablet"
									demandPopin="true">
								<Text text="Product"/>
							</Column>
							
								<Column
									minScreenWidth="Tablet"
									demandPopin="true">
								<Text text="Start Date"/>
							</Column>
							
								<Column
									minScreenWidth="Tablet"
									demandPopin="true">
								<Text text="End Date"/>
							</Column>
							
								<Column
									minScreenWidth="Tablet"
									demandPopin="true">
								<Text text="Unified DOI Total"/>
							</Column>
							
								<Column
