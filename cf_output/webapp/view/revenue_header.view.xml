		<f:header>
			<f:DynamicPageHeader pinnable="true">
				<f:content>
					<fb:FilterBar id="filterbar" reset="onReset" search="onSearch" useToolbar="false" showGoOnFB="true">
						<fb:filterItems>
							<fb:FilterItem name="A" label="Company">
								<fb:control>
									<Select
										id="slName"
										forceSelection="true"
										change="onSelectChange"
										items="{
                                                    path: '/SE_CMP_CODESet'
                                                }">
										<core:Item key="{cmp_code}" text="{cmp_code}"/>
									</Select>
								</fb:control>				
							</fb:FilterItem>								
							<fb:FilterItem name="B" label="Unified DOI Code">
								<fb:control>
									<Select
										id="slCategory"
										change="onSelectChange"
										forceSelection="true"
										>
										<core:Item key="" text="All"/>
										<core:Item key="1" text="1"/>
										<core:Item key="2" text="2"/>
										<core:Item key="3" text="3"/>
										<core:Item key="4" text="4"/>
										<core:Item key="5" text="5"/>
									</Select>
								</fb:control>				
							</fb:FilterItem>
							<fb:FilterItem name="C" label="Unified DOI Type">
								<fb:control>
									<Select
										id="slDOIType"
										change="onSelectChange"
										forceSelection="true"
										items="{
                                                    path: '/SE_DECKTYPESet'
                                                 }">
										<core:Item key="{DECKTYPE}" text="{DECKTYPE}"/>
									</Select>
								</fb:control>				
							</fb:FilterItem>
							<fb:FilterItem name="D" label="Venture">
								<fb:control>
									<Select
										id="slVenture"
										change="onSelectChange"
										forceSelection="true"
										items="{
                                                    path: '/SE_VENTURESet'
                                                }">
										<core:Item key="{Venture}" text="{Venture}"/>
									</Select>
								</fb:control>				
							</fb:FilterItem>
							<fb:FilterItem name="E" label="Well">
								<fb:control>
									<Select
										id="slWell"
										change="onSelectChange"
										forceSelection="true"
										items="{
                                                    path: '/SE_WELLNAMESet'
                                                }">
										<core:Item key="{WELLNAME}" text="{WELLNAME}"/>
									</Select>
								</fb:control>				
							</fb:FilterItem>
							
							<fb:FilterItem name="F" label="Product">
								<fb:control>
									<Select
										id="slProduct"
										change="onSelectChange"
										forceSelection="true"
										items="{
                                                    path: '/SE_PRODUCTSet'
                                                }">
										<core:Item key="{PRODUCT}" text="{PRODUCT}"/>
									</Select>
								</fb:control>				
							</fb:FilterItem>
							
						</fb:filterItems>
					</fb:FilterBar>
				</f:content>
			</f:DynamicPageHeader>
		</f:header>

		<!-- DynamicPage Content -->
		<f:content>
			<Table
					id="idProductsTable"
					inset="false"
					items="{
                            path: '/Revenue_HDRSet'
                        }"
                        selectionChange="onSelectRequestProgramList">
					<headerToolbar>
										<OverflowToolbar>
											<content>
												<Title text="Unified DOI Header" level="H2"/>
												<!--<ToolbarSpacer width="65em"/>-->
												<ToolbarSpacer/>
												<ComboBox id="idARPAPopinLayout" placeholder="Popin layout options" change="onPopinLayoutChanged">
